{
  "name": "github-discord-updater",
  "description": "Application to retrieve GitHub updates and send them through Discord webhooks",
  "version": "1.0.1",
  "author": "Juansecu",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "prettier": "prettier --write src/**/*.ts",
    "start:dev": "nodemon -q --watch ./src/**/*.ts ./src/main.ts",
    "start:prod": "node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Juansecu/github-discord-updater.git"
  },
  "keywords": [
    "discord",
    "discordjs",
    "github",
    "github-updates",
    "webhook"
  ],
  "bugs": {
    "url": "https://github.com/Juansecu/github-discord-updater/issues"
  },
  "homepage": "https://github.com/Juansecu/github-discord-updater#readme",
  "dependencies": {
    "discord.js": "^14.19.3",
    "express": "^5.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^22.15.30",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
